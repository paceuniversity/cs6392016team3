<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vjrene.cs.appdocknative3">

     <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">



        <activity
            android:name=".AppPageActivity"
            android:label="@string/title_activity_app_page"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SurveyActivity"
            android:label="@string/title_activity_survey"
            android:theme="@style/AppTheme.NoActionBar"
            android:parentActivityName=".AppPageActivity"
            />

        <activity
            android:name=".DeveloperActivity"
            android:label="@string/title_activity_developer"
            android:theme="@style/AppTheme.NoActionBar"
            android:parentActivityName=".AppPageActivity" />

        <activity
            android:name=".EducationActivity"
            android:label="@string/title_activity_education"
            android:theme="@style/AppTheme.NoActionBar"
            android:parentActivityName=".AppPageActivity"></activity>

    </application>

</manifest>